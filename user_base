import json
from abc import ABC,
abstractmethod

class UserBase(ABC):
     @abstractmethod
     def create_user(self,request:str)->str:
       pass
     @abstractmethod
     def list_users(self,request:str)->str:
       pass
     @abstractmethod
     def describe_user(self,request:str)->str:
       pass
     @abstractmethod
     def update_user(self,request:str)->str:
       pass
     @abstractmethod
     def get_user_teams(self,request:str)->str:
       pass
